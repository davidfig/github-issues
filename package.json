{
    "name": "github-issues",
    "displayName": "github-issues",
    "description": "Open Github Issues for workspace",
    "version": "1.0.0",
    "publisher": "yopeyopey",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Other"
    ],
    "repository": "git@github.com/davidfig/github-issues",
    "activationEvents": [
        "onCommand:extension.githubIssues"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.githubIssues",
                "title": "Github Issues"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "jsonfile": "^3.0.0"
    }
}
